  "description": "A simple post management package for Laravel and aslo the Laravel backpack for Admin panel.",
  "type": "library",
  "license": "MIT",
  "authors": [
      {
          "name": "Rotha SAMON",
          "email": "samonrotha@gmail.com"
      }
  ],
  "minimum-stability": "dev",
